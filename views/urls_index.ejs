<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
  <header>
      <% include ./partials/_header %>
  </header>
  <main>
    <div class="row">
      <div>
        <ul>
          <% for (let url in urls) { %>
              <% if (urls.hasOwnProperty(url)) { %>
                  <li ><a href="/urls/<%=`${url}`%>"><%= `http://localhost:8080/${url}` %></a> <%= `: ${urls[url]}` %></li>                
              <% } %>
            <% }; %>
      </ul>
      </div>
    </div>
  </main>
  </body>
</html>