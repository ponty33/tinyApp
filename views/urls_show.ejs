<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
  <header>
      <% include ./partials/_header %>
  </header>
  <main>
    <div class="row">
      <div>
        <ul>
          <% for (let url in urls) { %>
              <% if (url === shortURL) { %>
                  <li ><%= `${url}:` %> <%= `${urls[url]}` %></li>                
              <% } %>
            <% }; %>
      </ul>
      </div>
    </div>
  </main>
  </body>
</html>